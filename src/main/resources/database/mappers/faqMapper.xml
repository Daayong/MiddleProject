<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.d.mp.cs.faq.FaqDAO">
  
  <!-- list 불러오기 -->
  <select id="getList" resultType="com.d.mp.cs.faq.FaqDTO">
  	SELECT * FROM faq ORDER BY faq_id DESC
  </select>
  
  <!-- faq 글 추가 -->
  <insert id="setInsert" parameterType="com.d.mp.cs.faq.FaqDTO">
  	INSERT INTO faq VALUES(faq_seq.nextval, #{faq_type}, #{faq_subject}, #{faq_content})
  </insert>
  
  <!-- 유형별 list 보기 -->
 <!--  <select id="getTypeList" parameterType="com.d.mp.cs.faq.FaqDTO" resultType="com.d.mp.cs.faq.FaqDTO" >
  	SELECT * FROM faq WHERE faq_type like #{faq_type} ORDER BY faq_id DESC
  </select>
  -->
  
   </mapper>